#content {
    display: table;
}
 
#mainContent {
    display: table-cell;
    width: 620px;
    padding-right: 22px;
}
 
aside {
    display: table-cell;
    width: 300px;
}


#content table {
	border-collapse: collapse;
	width: 940px;
}

 #content table, #content th, #content td {
    width: 940px;
    padding: 15px;
    text-align: left;
    border-bottom: 1px solid #ddd;
}


th {
    background-color: #4CAF50;
    color: white;
}
