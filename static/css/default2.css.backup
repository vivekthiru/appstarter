    margin-top: 66px;
    padding: 44px;
    background: #467612 url("intro_background.png") repeat-x;
    background-size: 100%;
    border-radius: 22px;
}      


#page1 {
    margin-top: 66px;
    padding: 44px;
    background-size: 100%;
    border-radius: 22px;
}      


